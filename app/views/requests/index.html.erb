
<h1 class="title is-3">Requests you've made</h1>

<div class="container is-centered add-top-margin columns multiline">


<% @requests.each do |request| %>
<div class="column our-little-box box is-narrow">
<p><%= Listing.find(request.listing_id).title %></p>
<p><%= Listing.find(request.listing_id).description %></p>
<p><%= request.message %></p>
<p>Status:<%= request.status %></p>
</div>
<% end %>

</div>
</div>
